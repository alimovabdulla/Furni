
@{
    ViewData["Title"] = "edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model List<Team>
<!-- Start Hero Section -->
<div class="hero">
	<div class="container">
		<div class="row justify-content-between">
			<div class="col-lg-5">
				<div class="intro-excerpt">
					<h1   >About Us</h1>
					<p class="mb-4">Donec vitae odio quis nisl dapibus malesuada. Nullam ac aliquet velit. Aliquam vulputate velit imperdiet dolor tempor tristique.</p>
					<p><a href="#" class="btn btn-secondary me-2">Shop Now</a><a href="#" class="btn btn-white-outline">Explore</a></p>
				</div>
			</div>
			<div class="col-lg-7">
				<div class="hero-img-wrap">
					<img src=" " class="img-fluid">
				</div>
			</div>
		</div>
	</div>
</div>
<!-- End Hero Section -->
 


<div style="margin:50px" class="col-lg-5 mx-auto text-center">
	<h2 class="section-title">Our Team</h2>
</div>
<div class="untree_co-section product-section before-footer-section">
	<div class="container">

		<div class="row">


			@foreach (var item in Model)
			{


				<div class="col-12 col-md-6 col-lg-3 mb-5 mb-md-0">
					<h2>ID @item.id</h2>
					<img src="@item.ImgUrl" class="img-fluid mb-5">

					<h3 clas><a href="#"><span class="">@item.Name</span>.</a></h3>
					<span class="d-block position mb-4">@item.Status</span>
					<p>
						@item.Description
					</p>
					<p class="mb-0"><a href="#" class="more dark">Learn More <span class="icon-arrow_forward"></span></a></p>
					<span class="icon-cross">
						<a class="btn btn-primary" asp-controller="About" asp-action="update" asp-route-id="@item.id">Edit</a>
					</span>
					<span class="icon-cross">
						<a class="btn btn-primary" asp-controller="About" asp-action="delete" asp-route-id="@item.id">Delete</a>
					</span>

				</div>



			}





		</div>
	</div>
</div>

 